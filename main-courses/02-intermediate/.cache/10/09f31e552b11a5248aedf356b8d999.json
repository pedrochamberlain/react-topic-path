ges/SearchParams.js","original":{"line":39,"column":4}},{"generated":{"line":74,"column":0},"source":"pages/SearchParams.js","original":{"line":61,"column":1}},{"generated":{"line":74,"column":1},"source":"pages/SearchParams.js","original":{"line":7,"column":0}},{"name":"SearchParams","generated":{"line":76,"column":15},"source":"pages/SearchParams.js","original":{"line":63,"column":15}}],"sources":{"pages/SearchParams.js":"import React, { useState, useEffect } from 'react'\nimport pet, { ANIMALS as animals } from '@frontendmasters/pet'\n\nimport Results from '../components/Results'\nimport useDropdown from '../components/useDropdown'\n\nconst SearchParams = () => {\n    const [location, setLocation] = useState('Seattle, WA')\n    const [breeds, setBreeds] = useState([])\n\n    const [animal, AnimalDropdown] = useDropdown('Animal', 'dog', animals)\n    const [breed, BreedDropdown, setBreed] = useDropdown('Breed', '', breeds)\n    const [pets, setPets] = useState([])\n\n    async function requestPets() {\n        const { animals } = await pet.animals({\n            location,\n            breed,\n            type: animal\n        })\n\n        setPets(animals || [])\n    }\n\n    useEffect(() => {\n        setBreeds([])\n        setBreed('')\n\n        pet.breeds(animal)\n            .then(({ breeds: apiBreeds }) => {\n                const breedStrings = apiBreeds.map(({ name }) => name)\n                setBreeds(breedStrings)\n            })\n            .catch(() => {\n                console.error('Error while updating breeds')\n            })\n    }, [animal, setBreed, setBreeds])\n\n    return (\n        <div className=\"search-params\">\n            <form onSubmit={(e) => {\n                e.preventDefault()\n                requestPets()\n            }}>\n                <label htmlFor=\"location\">\n                    Location\n                    <input\n                        id=\"location\"\n                        value={location}\n                        placeholder=\"Location\"\n                        onChange={(e) => setLocation(e.target.value)}\n                    />\n                </label>\n                <AnimalDropdown />\n                <BreedDropdown />\n                <button>Submit</button>\n            </form>\n            <Results pets={pets} />\n        </div>\n    )\n}\n\nexport default SearchParams"},"lineCount":null}},"error":null,"hash":"fe49c6ad1fdc51992785eeca463fc538","cacheData":{"env":{}}}